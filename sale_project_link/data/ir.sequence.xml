<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <!-- Copy native "Sales Order" sequence into a template for project (used by this module)
             this is to keep native "Sales order" sequence clean and restore it at module uninstall 
        -->
        <record id="seq_sale_order_project_template" model="ir.sequence">
            <field name="name">Sales Order (project template)</field>
            <field name="code">sale.order.project.template</field>
            <field name="padding">3</field>
            <field name="implementation">standard</field>
            <field name="prefix">-S</field>
        </record>
    </data>
</odoo>
